---
import Layout from '../../layouts/global.astro';
import PageView from '../components/views/PageView.svelte';


interface Props {
  bannerNavAriaHeading: string;
  verticalNavAriaHeading: string;
}
const {
  bannerNavAriaHeading = '', // Hidden, Accessibility h2 heading for employee apps, nav region
  verticalNavAriaHeading = '', // Hidden heading to denote audience nav region for screen readers
} = Astro.props;


export { bannerNavAriaHeading, verticalNavAriaHeading };

---

<Layout title={"UW Health Launch Page"}>
  <PageView bannerNavAriaHeading={bannerNavAriaHeading} verticalNavAriaHeading={verticalNavAriaHeading} client:load />
</Layout>
